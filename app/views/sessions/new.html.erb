<%= form_tag signin_path, method: :post do %>
  <%= label_tag :name %> <br>
  <%= select_tag :name, options_from_collection_for_select(User.all, :name, :name) %><br>
  <%= label_tag :password %> <br>
  <%= password_field_tag(:password, nil, class: "form-control") %>
  <%= submit_tag "Sign In" %> <br>
<% end %>
